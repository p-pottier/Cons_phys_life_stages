@article{ WOS:000329148900007,
Author = {Burger, Mark and van Breukelen, Frank},
Title = {Construction of a low cost and highly sensitive direct heat calorimeter
   suitable for estimating metabolic rate in small animals},
Journal = {JOURNAL OF THERMAL BIOLOGY},
Year = {2013},
Volume = {38},
Number = {8},
Pages = {508-512},
Month = {DEC},
Abstract = {Although the concept of a metabolic rate is readily understood, actual
   measurement of metabolism has proved much more difficult. The numerous
   strategies for estimation of metabolic rate all result in an incomplete
   accounting. Respirometry or gas exchange is the most widely used
   approach but mostly ignores the anaerobic component. Here, we describe a
   readily-built and low cost direct heat calorimeter that may be coupled
   with standard respirometry equipment to provide a more complete portrait
   of metabolism. The device is sensitive and provides a predictable
   measurement of heat flow from an organism. (C) 2013 Elsevier Ltd. All
   rights reserved.},
