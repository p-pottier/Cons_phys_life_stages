@article{ WOS:000366775500017,
Author = {Diaz, Fernando and Re, Ana Denisse and Salas, Alfredo and
   Galindo-Sanchez, Clara E. and Gonzalez, Marco A. and Sanchez, Adolfo and
   Rosas, Carlos},
Title = {Behavioral thermoregulation and critical thermal limits of giant keyhole
   limpet <i>Megathura crenulata</i> (Sowerby 1825) (Mollusca;
   Vetigastropoda)},
Journal = {JOURNAL OF THERMAL BIOLOGY},
Year = {2015},
Volume = {54},
Number = {SI},
Pages = {133-138},
Month = {DEC},
Abstract = {The thermoregulatory behavior of the giant keyhole limpet Megathura
