   Cooley, Nikita L.; Emlen, Douglas J.; Woods, H. Arthur, Univ Montana, Div Biol Sci, 32 Campus Dr HS104, Missoula, MT 59812 USA.},
DOI = {10.1016/j.jtherbio.2016.10.002},
ISSN = {0306-4565},
Keywords = {Heat balance; Body size; Metabolic heat; Critical thermal maxima;
   Behavioral thermoregulation; Gigantism; Soil},
