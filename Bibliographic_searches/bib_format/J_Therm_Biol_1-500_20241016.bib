@article{ WOS:000466820900019,
Author = {Sukumar, S. and Kar, S. P.},
Title = {Numerical analysis of an enhanced cooling rate cryopreservation process
   in a biological tissue},
Journal = {JOURNAL OF THERMAL BIOLOGY},
