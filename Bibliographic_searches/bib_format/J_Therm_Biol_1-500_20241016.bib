@article{ WOS:000497246100002,
Author = {Martinez-Juarez, Adriana and Moreno-Mendoza, Norma},
Title = {Mechanisms related to sexual determination by temperature in reptiles},
Journal = {JOURNAL OF THERMAL BIOLOGY},
Year = {2019},
Volume = {85},
Month = {OCT},
Abstract = {A number of strategies have emerged that appear to relate to the
   evolution of mechanisms for sexual determination in vertebrates, among
   which are genetic sex determination caused by sex chromosomes and
   environmental sex determination, where environmental factors influence
   the phenotype of the sex of an individual. Within the reptile group,
   some orders such as: Chelonia, Crocodylia, Squamata and Rhynchocephalia,
   manifest one of the most intriguing and exciting environmental sexual
   determination mechanisms that exists, comprising temperature-dependent
   sex determination (TSD), where the temperature of incubation that the
   embryo experiences during its development is fundamental to establishing
   the sex of the individual. This makes them an excellent model for the
   study of sexual determination at the molecular, cellular and
   physiological level, as well as in terms of their implications at an
   evolutionary and ecological level. There are different hypotheses
   concerning how this process is triggered and this review aims to
   describe any new contributions to particular TSD hypotheses, analyzing
