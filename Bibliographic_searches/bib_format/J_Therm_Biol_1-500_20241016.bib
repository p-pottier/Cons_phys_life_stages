@article{ WOS:000518869900018,
Author = {Oke, O. E. and Alo, E. T. and Oke, F. O. and Oyebamiji, Y. A. and
   Ijaiya, M. A. and Odefemi, M. A. and Kazeem, R. Y. and Soyode, A. A. and
   Aruwajoye, O. M. and Ojo, R. T. and Adeosun, S. M. and Onagbesan, O. M.},
Title = {Early age thermal manipulation on the performance and physiological
   response of broiler chickens under hot humid tropical climate},
Journal = {JOURNAL OF THERMAL BIOLOGY},
Year = {2020},
Volume = {88},
Month = {FEB},
Abstract = {Initial brooding temperature is critical for post-hatch growth of
   broiler chickens. A study was conducted to investigate the early age
   thermal manipulation (EATM) on the performance and physiological
   responses broiler chickens under hot humid tropical climate. A total of
   260 unsexed day-old Arbor-acre broiler chicks were assigned to five
   thermal treatments of brooding temperature regimens having 4 replicates
   of thirteen birds each. The heat treatments were: initial brooding
   temperature of 35 degrees C for the first 2 days, and then decreased
   subsequently, gradually to 22 degrees C at 21 d of age (CT), initial
   temperature of 35 degrees C, sustained for the first 4 days and then
   decreased gradually (conventionally) (FD), initial temperature of 35
   degrees C for the first 7 days (SD), the birds in CT, but the brooding
   temperature was raised to 35 degrees C again for another 3 days from day
   7 (SD3), initial brooding temperature of 35 degrees C for the first 10
   days (TD). Data were collected on daily feed intake and weekly body
   weights. Blood samples were collected from 8 birds per treatment weekly
   for the determination of plasma uric acid, triglycerides,
   triiodothyronine (T-3) and creatinine kinase. Data obtained were laid
   out in a Completely Randomized Design (CRD). Results showed that the
   final weights of the birds in FD were higher (P < 0.05) than those of
   the other treatments at the finisher phase. Feed intake of the birds in
   FD was higher than those of SD3 and TD. FCR of broiler thickens in CT,
   SD, SD3 and TD was higher than that of FD. The rectal temperature,
   plasma MDA and blood glucose of the thermally challenged birds in FD was
   generally better (P < 0.05) than those of the other treatments. It was
   concluded that EATM can be used to improve performance and also protect
   broiler chickens from acute heat stress at market age.},
