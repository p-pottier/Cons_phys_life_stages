
@article{ WOS:000598118200006,
Author = {da Silva, Marcos Vinicius and Pandorfi, Heliton and Pontes de Almeida,
   Gledson Luiz and da Rosa Ferraz Jardim, Alexandre Manicoba and Dias
   Batista, Pedro Henrique and Batista da Silva, Rodes Angelo and Lopes,
