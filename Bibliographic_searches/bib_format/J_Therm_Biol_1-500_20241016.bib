@article{ WOS:000497246100009,
Author = {Khosravi, Maral and Nasrolahi, Ali and Shokri, Mohammad Reza and
   Dobretsov, Sergey and Pansch, Christian},
Title = {Impact of warming on biofouling communities in the northern Persian Gulf},
Journal = {JOURNAL OF THERMAL BIOLOGY},
Year = {2019},
Volume = {85},
Month = {OCT},
Abstract = {While the impact of ocean warming on single species is well described,
   the impact on marine biofouling communities is not well understood.
   Effluents of power plants have higher temperatures and can be used as
   natural large-scale test sites to investigate warming effects on marine
   ecosystems. In the present study, we evaluated the impact of elevated
   temperatures in the vicinity of a power plant on macro-biofouling
   communities in the northern coast of the Persian Gulf. The impact site
   was on average 2 degrees C warmer than the control site. Our results
   demonstrate a significantly different structure and composition of
   biofouling communities between control and impact sites. Warming led to
   a 1.5-fold increase in the mean coverage of biofouling communities and
   slightly decreased functional and species richness. Our results
   indicated that future warming will likely increase biofouling pressure,
   while decreasing diversity of communities, particularly in habitats
   where organisms exist at their upper tolerance limits of temperature.},
