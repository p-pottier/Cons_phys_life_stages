@article{ WOS:000552838300002,
Author = {Schneider, Marion and Ziegler, Thomas and Kolter, Lydia},
Title = {Thermoregulation in Malayan sun bears (<i>Helarctos malayanus</i>) and
   its consequences for <i>in situ</i> conservation},
Journal = {JOURNAL OF THERMAL BIOLOGY},
Year = {2020},
Volume = {91},
Month = {JUL},
Abstract = {Thermoregulation in Malayan sun bears is not fully understood.
   Therefore, in this study the effect of meteorological variables on both
   behavioural and autonomic thermoregulatory mechanisms in sun bears was
   examined in order to identify temperature thresholds for the activation
   of various thermoregulatory mechanisms. Infrared thermography was used
   to non-invasively determine body surface temperature (T-S) distribution
   in relation to ambient temperature (T-A) and to determine the
   thermoneutral zone (TNZ) of sun bears. Thermographic measurements were
   performed on 10 adult sun bears at T-A between 5 degrees C and 30
   degrees C in three European zoos. To assess behaviours that contribute
   to thermoregulation, nine adult sun bears were observed at T-A ranging
   from 5 degrees C to 34 degrees C by instantaneous scan sampling in 60 s
   intervals for a total of 787 h. Thermographic measurements revealed that
   the TNZ of sun bears lies between 24 degrees C and 28 degrees C and that
   heat is equally dissipated over the body surface. Behavioural data
   showed that behaviours related to thermoregulation occurred in advance
   of energetically costly autonomic mechanisms, and were highly correlated
   with T-A and solar radiation. While the temperature threshold for the
   onset of thermoregulatory behaviours below the TNZ lies around 15
   degrees C, which is well below the lower critical temperature (T-LC)
   assessed by thermography, the onset for behaviours to prevent
   overheating occurred at 28 degrees C, which was closer to the estimated
   upper critical temperature (T-UC) of sun bears. These findings provide
   useful data on the thermal requirements of sun bears with respect to the
   species potential to cope with the effects of climate change and
   deforestation which are occurring in their natural range. Furthermore,
   these results may have important implications for the care and welfare
   of bears in captivity and should be taken into consideration, when
   designing and managing facilities.},
