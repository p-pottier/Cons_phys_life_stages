@article{ WOS:000341477900009,
Author = {Singh, Rupesh and Das, Koushik and Mishra, Subhash C.},
Title = {Laser-induced hyperthermia of nanoshell mediated vascularized tissue - A
   numerical study},
Journal = {JOURNAL OF THERMAL BIOLOGY},
Year = {2014},
Volume = {44},
Pages = {55-62},
Month = {AUG},
Abstract = {Laser-induced hyperthermia treatment of tumor in a 2-D axisymmetric
   tissue embedded with moderate size (100-150 mu m) blood vessels is
   studied. Laser absorption is enhanced by embedding gold-silica
   nanoshells in the tumor. Heat transfer in the tissue is modeled using
   Weinbaum-Jiji bioheat transfer equation. With laser irradiation, the
   volumetric radiation is accounted in the governing bioheat equation.
   Radiative information needed in the bioheat equation is calculated using
   the discrete ordinate method, and the coupled bioheat-radiation equation
   is solved using the finite volume method. Effects of power density,
   laser exposure time, beam radius, diameter of blood vessel and volume
   fractions of nanoshells on temperature spread in the tissue are
   analyzed. (C) 2014 Elsevier Ltd. All rights reserved.},
