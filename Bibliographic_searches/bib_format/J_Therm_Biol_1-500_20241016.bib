@article{ WOS:000329148900009,
Author = {Liu, Junying and Yan, Xiping and Li, Qingqing and Wang, Guosong and Liu,
   Hehe and Wang, Jiwen and Li, Liang and Du, Xiaohui and Han, Chunchun and
   He, Hua},
Title = {Thermal manipulation during the middle incubation stage has a repressive
   effect on the immune organ development of Peking ducklings},
Journal = {JOURNAL OF THERMAL BIOLOGY},
Year = {2013},
Volume = {38},
Number = {8},
Pages = {520-523},
Month = {DEC},
Abstract = {Avian embryos are easily influenced by their environment during
   incubation. Previous studies have demonstrated that incubation
   temperature changes could influence muscle development and body weight,
   which subsequently determine the adult phenotype. The objective of this
   study was to investigate whether the development of immune organs in
   ducklings could be influenced by thermal manipulation during the middle
   stage of incubation. To evaluate this hypothesis, a control group was
   incubated under a normal temperature from Ell to E24, while the
   incubation temperature of the experimental group was increased by 1 C.
   Our results indicated that slight changes in the incubation temperature
   significantly repressed the bursa of Fabricius index of the duck embryo
   on E25 (F1, 58=122.51, P <0.0001) and significantly repressed the spleen
   index of neonatal ducklings (F1, 58=74.38, P <0.0001). At 0 day
   posthatching (dph) and 14 dph, ducklings hatched from eggs incubated
   under the higher temperature had a lower percentage of globulin than the
   control group (F1, 10=19.97, P=0.0111; Fl, 10=9.8, P=0.0352). The IFN-y
   concentration of ducklings at 14 dph displayed the same trend (Fl,
   10=284.49, P < 0.0001). These results suggested that thermal
   manipulation during the middle stage of incubation had a repressive
   effect on the development of immune organs and reduced the
   concentrations of serum globulin and IFN-y. These results demonstrated
   that the subtle alteration of incubation temperature may weaken
   ducklings' immunity. (C) 2013 Elsevier Ltd. All rights reserved.},
