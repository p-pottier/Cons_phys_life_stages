@article{ WOS:000329148900002,
Author = {Hadzi-Petrushev, Nikola and Mladenov, Kliment and Sopi, Ramadan and
   Stojkovski, Velimir and Mitrov, Dine and Ilieski, Vlatko and Mladenov,
   Mitko},
Title = {Enhanced lipid peroxidation and inflammation during heat exposure in
   rats of different ages: Role of α-tocopherol},
Journal = {JOURNAL OF THERMAL BIOLOGY},
Year = {2013},
Volume = {38},
Number = {8},
Pages = {474-479},
Month = {DEC},
Abstract = {To investigate early events possibly related to the development of heat
   shock, we examined whether inflammatory-(interleukin-6, tumor necrosis
   factor alpha and 15-keto-13,14-dihydro-PGF(2 alpha)) and
   peroxidative-(8-iso-PGF(2 alpha) and malondialdehyde) markers are
   altered during acute heat exposure and aging. We also studied the
   relationships between inflammatory and peroxidative markers in these
   settings. In order to prevent these reactions developed as a consequence
   of the conditions mentioned above, we tested the effects of
   alpha-tocopherol. Our results demonstrated that
   15-keto-13,14-dihydro-PGF2(alpha) and malondialdehyde in the liver were
   altered during acute heat exposure in The young and middle-aged rats and
   could be predicted by changes in the levels of circulatory cytokines.
   Regardless of age, the supplementation with alpha-tocopherol prevented
   changes in the plasma cytokine levels and 15-keto-13,14-dihydro-PGF(2
   alpha) and malondialdehyde levels in the liver, during acute heat
   exposure. This study notably emphasized the ability of a-tocopherol to
   prevent different heat induced mechanisms, involved in induction of
   inflammatory or peroxidative reactions. (C) 2013 Elsevier Ltd. All
   rights reserved.},
