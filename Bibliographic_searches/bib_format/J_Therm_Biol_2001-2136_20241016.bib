   Kim, Won Seob, Michigan State Univ, Dept Anim Sci, E Lansing, MI 48824 USA.
   Kim, Jongkyoo, Michigan State Univ, Anim Sci \& Food Sci \& Human Nutr, E Lansing, MI 48824 USA.},
DOI = {10.1016/j.jtherbio.2023.103684},
