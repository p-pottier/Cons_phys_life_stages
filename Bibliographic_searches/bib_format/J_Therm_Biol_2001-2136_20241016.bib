@article{ WOS:000466820900018,
Author = {Guo, Yang-Yang and Chi, Qing-Sheng and Zhang, Xue-Ying and Liu, Wei and
   Hao, Shao-Yan and Wang, De-Hua},
Title = {Brown adipose tissue plays thermoregulatory role within the
   thermoneutral zone in Mongolian gerbils (<i>Meriones unguiculatus</i>)},
Journal = {JOURNAL OF THERMAL BIOLOGY},
Year = {2019},
Volume = {81},
Pages = {137-145},
Month = {APR},
Abstract = {Brown adipose tissue (BAT) plays an important role in thermoregulation
   and many metabolic processes in small mammals, especially in cold
   adaptation. However, in warm adaptation, ambient temperature cannot
   directly activate BAT by sympathetic nervous system. Mongolian gerbils
   exhibit a wider thermoneutral zone (26.5-38.9 degrees C). We
   hypothesized that BAT atrophied near the lower critical temperature and
   further atrophied near the upper critical temperature. Male gerbils were
   acclimated to 23 degrees C, 27 degrees C or 37 degrees C, respectively,
   for 3 weeks. Results showed that regulatory non-shivering thermogenesis
   did not change in gerbils acclimated to 27 degrees C compared with 23
   degrees C group, whereas it was reduced by 43.5\% in gerbils acclimated
   to 37 degrees C. Bigger lipid droplet in BAT was observed in gerbils
   acclimated to 27 degrees C and 37 degrees C compared with 23 degrees C
   group, while the expression of uncoupling protein 1 and tyrosine
   hydroxylase was only reduced in gerbils acclimated to 37 degrees C.
   Further, thermoneutral acclimation did not change BAT thermogenesis by
   down-regulation of peroxisome proliferator-activated receptor gamma
   coactivator-1 alpha, PR domain containing 16, peroxisome
   proliferator-activated receptor-alpha or peroxisome proliferator
   activated receptor-gamma gene expression in BAT. In addition, body
   temperature was reduced in gerbils acclimated to 37 degrees C compared
   with 23 degrees C group, which was associated with a decreased resting
   metabolic rate and regulatory non-shivering thermogenesis. In
   conclusion, BAT does not atrophy near the lower critical temperature,
   whereas it atrophies near the upper critical temperature, suggesting
   that BAT may play thermoregulatory role within the TNZ in Mongolian
   gerbils.},
