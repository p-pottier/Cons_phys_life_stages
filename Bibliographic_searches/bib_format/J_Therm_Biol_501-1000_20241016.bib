@article{ WOS:000390721000006,
Author = {Liu, Kuo-Chi and Chen, Han-Taw and Cheng, Po-Jen},
Title = {Inverse investigation of non-Fourier heat conduction in tissue},
Journal = {JOURNAL OF THERMAL BIOLOGY},
