@article{ WOS:000329148900004,
Author = {Moore, T. and Sorokulova, I. and Pustovyy, O. and Globa, L. and
   Vodyanoy, V.},
Title = {Microscopic evaluation of vesicles shed by rat erythrocytes at elevated
   temperatures},
Journal = {JOURNAL OF THERMAL BIOLOGY},
Year = {2013},
Volume = {38},
Number = {8},
Pages = {487-492},
Month = {DEC},
Abstract = {It is hypothesized that the elevation of the temperature of the blood
   during heat stress may cause an increase of the shedding of erythrocyte
   membrane vesicles. Therefore, the increase of vesicle numbers following
   heat stress may be indicative of and proportional to the level heat
   stress. In order to test this hypothesis, erythrocytes and the vesicles
   shed by erythrocytes were collected from rat blood and analyzed after
   the elevation of body temperature by exposure to external heat. The
   images of erythrocytes and vesicles were analyzed by a custom light
   microscopy system with spatial resolution of better than 90 nm. The
   samples were observed in an aqueous environment and required no
   freezing, dehydration, staining, shadowing, marking or any other
   manipulation. The elevation of temperature from 36.7 +/- 0.3 to 40.3 +/-
   0.4 degrees C resulted in significant increase of the concentration of
   vesicles in blood. At a temperature of 37 degrees C, mean vesicle
   concentrations and diameters found in rat blood were (1.4 +/- 0.2) x
   10(6) vesicles/mu L and 0.436 +/- 0.03 mu m, respectively. The
   concentration of free vesicles increased after exposure to heat to (3.8
   +/- 0.3) x 10(6) vesicles/mu L. It was estimated that 80\% of all
   vesicles found in rat blood are smaller than 0.45 mu m. The increase in
   the number of vesicle associated with elevated temperatures may be
   indicative of the heat stress level and serve as diagnostic test of
   erythrocyte stability and heat resistance. (C) 2013 Elsevier Ltd. All
   rights reserved.},
