@article{ WOS:000609163700007,
Author = {Gilbert, Matthew J. H. and Farrell, Anthony P.},
Title = {The thermal acclimation potential of maximum heart rate and cardiac heat
   tolerance in Arctic char (<i>Salvelinus alpinus</i>), a northern
   cold-water specialist},
Journal = {JOURNAL OF THERMAL BIOLOGY},
Year = {2021},
Volume = {95},
Month = {JAN},
Abstract = {Increasing heart rate (f(H)) is a central, if not primary mechanism used
   by fishes to support their elevated tissue oxygen consumption during
   acute warming. Thermal acclimation can adjust this acute response to
   improve cardiac performance and heat tolerance under the prevailing
   temperatures. We predict that such acclimation will be particularly
   important in regions undergoing rapid environmental change such as the
   Arctic. Therefore, we acclimated Arctic char (Salvelinus alpinus), a
   high latitude, cold-adapted salmonid, to ecologically relevant
   temperatures (2, 6, 10, 14 and 18 degrees C) and examined how thermal
   acclimation influenced their cardiac heat tolerance by measuring the
   maximum heart rate (f(Hmax)) response to acute warming. As expected,
   acute warming increased f(Hmax) in all Arctic char before f(Hmax)
   reached a peak and then became arrhythmic. The peak f(Hmax), and the
   temperature at which peak fax (T-peak) and that at which arrhythmia
   first occurred (T(ar)r) all increased progressively (+33\%, 49\% and
   35\%, respectively) with acclimation temperature from 2 to 14 degrees C.
   When compared at the same test temperature fax also decreased by as much
   as 29\% with increasing acclimation temperature, indicating significant
   thermal compensation. The upper temperature at which fish first lost
   their equilibrium (critical thermal maximum: CTmax) also increased with
   acclimation temperature, albeit to a lesser extent (+11\%). Importantly,
   Arctic char experienced mortality after several weeks of acclimation at
   18 degrees C and survivors did not have elevated cardiac thermal
   tolerance. Collectively, these findings suggest that if wild Arctic char
   have access to suitable temperatures (<18 degrees C) for a sufficient
   duration, warm acclimation can potentially mitigate some of the
   cardiorespiratory impairments previously documented during acute heat
