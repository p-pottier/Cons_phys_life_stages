@article{ WOS:000456758000007,
Author = {Abdel-Ghany, Heba M. and El-Sayed, Abdel-Fattah M. and Ezzat, Altaf A.
   and Essa, Mohamed A. and Helal, Amr M.},
Title = {Dietary lipid sources affect cold tolerance of Nile tilapia
   (<i>Oreochromis niloticus</i>)},
