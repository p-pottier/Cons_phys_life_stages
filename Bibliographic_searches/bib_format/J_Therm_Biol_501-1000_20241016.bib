@article{ WOS:000317949400006,
Author = {Wang, Yunxia and Zhang, Shuhua and Ge, Yuzhi and Liu, Yanfeng and Yang,
   Dongmei and Luo, Jun and Wu, Zhiting},
Title = {Role of BK<sub>Ca</sub> channels in vascular smooth muscle during the
