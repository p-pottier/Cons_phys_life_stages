@article{ WOS:000399513900019,
Author = {Kir, Mehmet and Sunar, Murat Can and Altindag, Baris Can},
Title = {Thermal tolerance and preferred temperature range of juvenile meagre
   acclimated to four temperatures},
Journal = {JOURNAL OF THERMAL BIOLOGY},
Year = {2017},
Volume = {65},
Pages = {125-129},
Month = {APR},
Abstract = {The present study reports the temperature tolerance, estimated using
   dynamic and static methodologies, and preferred temperature range, based
   on oxygen consumption rate (OCR), of juvenile meagre (Argyrosomus
   regius) (Asso, 1801) (3.4 +/- 0.9 g) after 30 days of acclimation at 18,
   22, 26 and 30 degrees C. Meagre has dynamic and static thermal tolerance
   zones of 551 degrees C-2 and 460 degrees C-2, respectively and is a low
   resistance fish species, with a resistance zone area of 87 degrees C-2.
   The OCR of juvenile meagre at the above acclimation temperatures was
   370, 410, 618 and 642 mg h(-1) kg(-1), respectively, and is
   significantly different (P < 0.0001, n=20). The fact that OCR increases
   by rising temperatures and gradually decreases after 26 degrees C
   indicates that the preferred temperature range of juvenile meagre is
   between 26 and 30 degrees C. Our study suggests that meagre is unable to
   respond to low and high temperature variation in aquaculture facilities
   or its natural habitats.},
