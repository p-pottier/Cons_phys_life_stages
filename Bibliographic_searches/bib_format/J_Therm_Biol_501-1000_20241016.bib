@article{ WOS:000394066600009,
Author = {Ngoula, Ferdinand and Tekam, Maryvonne Guemdjo and Kenfack, Augustave
   and Tchingo, Cyrille D'Alex Tadondjou and Nouboudem, Sandrine and
   Ngoumtsop, Herman and Tsafack, Borice and Teguia, Alexis and
   Kamtchouing, Pierre and Galeotti, Marco and Tchoumboue, Joseph},
Title = {Effects of heat stress on some reproductive parameters of male cavie
   (<i>Cavia porcellus</i>) and mitigation strategies using guava
   (<i>Psidium guajava</i>) leaves essential oil},
Journal = {JOURNAL OF THERMAL BIOLOGY},
Year = {2017},
Volume = {64},
Pages = {67-72},
Month = {FEB},
Abstract = {Climate changes, particularly the increase of temperature are among the
   main causes behind the decline of fertility in humans as well as
   animals. In this study, the effects of heat stress on some reproductive
   parameters of male cavies and mitigation strategies using guava leaves
   essential oil (GLEO) were studied. For this purpose, 40 male cavies aged
   2.5-3 months and weighing between 348 and 446 g were divided into 4
   groups of 10 animals each and subjected to the following temperatures:
   Ambient temperature (20-25 degrees C) for the control group, 35 degrees
   C for group 1, 45 degrees C for group 2 and 45 C+100 mu I GLEO/kg body
   weight, administered by gavage to animals for group 3. Exposure time of
   heat was 7 h per day for 60 days. Results reveal that the relative
   weights of testes, epididymis, vas deferens and seminal vesicles were
   hardly affected by the temperature levels considered (P > 0.05). The
   mass and individual sperm motility was significantly lower (P < 0.05) in
   cavies exposed to the temperature of 35 and 45 degrees C as compared
   with those which received GLEO and controls. The percentages of abnormal
   sperm and altered sperm DNA were higher in animals expbsed to
   temperature of 35 and 45 degrees C as compared with the controls. The
   activity of superoxide dismutase significantly increased (P < 0.05) in
   animals exposed to temperature of 45 degrees C and in those of 45
   degrees C and orally treated with GLEO, compared with cavies exposed to
   temperature of 45 degrees C without receiving GLEO. The level of
   malondialdehyde was significantly increased (P < 0.05) in animals
   exposed to temperature of 35 and 45 degrees C, whereas the level of
   nitric oxide was significantly lower (P < 0.05) in exposed animals as
   compared with controls. It was concluded that the exposure of male
   cavies at 35 and 45 degrees C for 60 days induce heat stress that causes
   deterioration of sperm characteristics. These effects that can be
   mitigated by the administration of guava leaves essential oil.},
