@article{ WOS:000314446300002,
Author = {Mellish, J. and Nienaber, J. and Polasek, L. and Horning, M.},
Title = {Beneath the surface: Profiling blubber depth in pinnipeds with infrared
   imaging},
Journal = {JOURNAL OF THERMAL BIOLOGY},
