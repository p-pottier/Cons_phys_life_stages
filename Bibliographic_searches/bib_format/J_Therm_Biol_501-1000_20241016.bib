@article{ WOS:000439400800013,
Author = {Weigert, Martin and Nitzsche, Nico and Kunert, Felix and Loesch,
   Christiane and Schulz, Henry},
Title = {The influence of body composition on exercise-associated skin
   temperature changes after resistance training},
Journal = {JOURNAL OF THERMAL BIOLOGY},
