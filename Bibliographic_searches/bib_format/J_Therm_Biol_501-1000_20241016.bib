@article{ WOS:000787901500004,
Author = {Walsh, Benjamin S. and Parratt, Steven R. and Snook, Rhonda R. and
   Bretman, Amanda and Atkinson, David and Price, Tom A. R.},
Title = {Female fruit flies cannot protect stored sperm from high temperature
   damage},
Journal = {JOURNAL OF THERMAL BIOLOGY},
