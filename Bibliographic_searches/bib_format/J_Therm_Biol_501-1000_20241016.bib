@article{ WOS:000466820900013,
Author = {Ivanov, I. T. and Chakaarov, I and Chakaarova, P.},
Title = {Thermal sensitivity and haemolysis of erythrocytes with membranopathy},
Journal = {JOURNAL OF THERMAL BIOLOGY},
Year = {2019},
Volume = {81},
Pages = {98-102},
Month = {APR},
Abstract = {Measuring the impedance of heated suspensions of erythrocytes and
   erythrocyte ghost membranes, two thermally-induced alterations are
   registered in the plasma membrane at T-A (denaturation of spectrin with
   inducing temperature at 49,5 degrees C) and T-G (hyperthermic activation
   of basal ion permeability with inducing temperature at 60.7 degrees C).
   In this study erythrocytes from 9 healthy patients and 15 patients with
   hemolytic anemia were studied and divided into four groups depending on
   their T-A and T-G top temperatures. The T-A and T-G of erythrocytes with
   hemoglobinopathy were the same as those of control erythrocytes while
   those of erythrocytes with membranopathy were significantly reduced. In
   erythrocytes with severe membranopathy, the T-G was decreased by about 5
   degrees C. In latter cells the normal value of T-G was restored and the
   resistance to thermal haemolysis was increased by 90\% after the
   specific stabilization of band 3 protein by
   4,4'-diisothiocyanato-stilbene-2,2'-disulfonic acid (DIDS). Obtained
   results indicate the involvement of band 3 in the membrane alteration at
   T-G and in the heat target responsible for thermal haemolysis.},
