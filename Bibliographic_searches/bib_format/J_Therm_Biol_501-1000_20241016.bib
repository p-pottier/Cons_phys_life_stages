@article{ WOS:000325043800011,
Author = {Liu, Yanfeng and Wang, Lijuan and Liu, Jiaping and Di, Yuhui},
Title = {A study of human skin and surface temperatures in stable and unstable
   thermal environments},
Journal = {JOURNAL OF THERMAL BIOLOGY},
