@article{ WOS:000329148900019,
Author = {Imbeault, Marie-Andree and Mantha, Olivier L. and Haman, Francois},
Title = {Shivering modulation in humans: Effects of rapid changes in
   environmental temperature},
Journal = {JOURNAL OF THERMAL BIOLOGY},
Year = {2013},
Volume = {38},
Number = {8},
Pages = {582-587},
Month = {DEC},
Abstract = {During cold exposure, increase in heat production is produced via the
   activation of shivering thermogenesis and nonshivering thermogenesis,
   the former being the main contributor to compensatory heat production in
   non-acclimatized humans. In rats, it has been demonstrated that
   shivering thermogenesis is modulated solely by skin thermoreceptors but
   this modulation has yet to be investigated in humans. The aim of this
   study was to determine if cold-induced shivering in humans can be
   modulated by cutaneous thermoreceptors in conditions where increases in
   heat loss can be adequately compensated by increases in thermogenic
   rate. Using a liquid-conditioned suit, six non-acclimatized men were
   exposed to cold (6 degrees C) for four 30 min periods, each of them
   separated by 15 min of heat exposure (33 degrees C). Core temperature
   remained stable throughout exposures whereas skin temperatures
   significantly decreased by 12\% in average during the sequential
   cold/heat exposures compared to baseline (p < 0.0001). Shivering
   intensity and metabolic rate increased significantly during 6 degrees C
   exposures (3.3 +/- 0.7\% MVC, 0.40 +/- 0.0 L O-2/min, respectively) and
   were significantly reduced during 33 degrees C exposure (0.5 +/- 0.1\%
   MVC, 0.25 +/- 0.0 L O-2/min; p < 0.005 for both). Most importantly,
   shivering could be quickly and strongly inhibited during 33 degrees C
   exposure although skin temperature often remained below baseline values.
   In conclusion, under compensatory conditions, cutaneous thermoreceptors
   appear to be a major modulator of the shivering response in humans and
   seem to react rapidly to changes in the microclimate right next to the
   skin and to skin temperature. (C) 2013 Elsevier Ltd. All rights
   reserved.},
