@article{ WOS:000787901500009,
Author = {Bakken, George S. and Tillman, Francis E. and O'Keefe, Joy M.},
Title = {Methods for assessing artificial thermal refuges: Spatiotemporal
   analysis more informative than averages},
Journal = {JOURNAL OF THERMAL BIOLOGY},
Year = {2022},
Volume = {105},
Month = {APR},
Abstract = {1: Thermal refuges are widely used by animals of all taxonomic groups
   and are critical to survival in severe weather.\& nbsp;2: Human
   activities are reducing the availability of natural refuges;
   consequently, artificial refuges are used as conservation management
   tools, particularly for bats.\& nbsp;3: Published box evaluations are
   generally incomplete, omitting thermal physiology and relevant thermal
   properties.\& nbsp;4: Here, we compare methods for evaluating the
   potential utility of bat box designs for bats and present a graphical
   spatiotemporal method that provides more complete information.\& nbsp;5:
   For illustration, we compare the original to three modified versions of
   the ``rocket box `` style bat box.\& nbsp;6: Box internal temperatures
   and generalized thermal physiology models are combined in two
   suitability indices appropriate to the mother, and to pups.\& nbsp;7:
   Results revealed that daily and seasonal averages of these indices
   obscured important processes and showed insignificant differences among
   bat box design modifications.\& nbsp;8: In contrast, graphical analysis
   highlighted the presence and spatiotemporal structure of significant
   differences among boxes, which were most evident in sunny weather.\&
   nbsp;9: Differences among boxes were sensitive to assumptions about bat
   thermal physiology and behavior.\& nbsp;10: We found that an external
   water jacket mitigated temperature extremes and extended favorable
   temperatures into the night, which could enhance pup development while
   the mothers foraged.\& nbsp;11: Further experiments are needed to
   evaluate the relation between metabolic heating by box occupants and
   thermal conditions within bat boxes.},
