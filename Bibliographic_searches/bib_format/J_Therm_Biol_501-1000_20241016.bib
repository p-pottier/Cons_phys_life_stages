@article{ WOS:000423637400006,
Author = {Dutta, Jaideep and Kundu, Balaram},
Title = {Two-dimensional closed-form model for temperature in living tissues for
   hyperthermia treatments},
Journal = {JOURNAL OF THERMAL BIOLOGY},
