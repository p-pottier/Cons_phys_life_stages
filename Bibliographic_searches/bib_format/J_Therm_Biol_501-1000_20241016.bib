@article{ WOS:000389117300006,
Author = {Kalinnikova, Tatiana B. and Kolsanova, Rufina R. and Belova, Evgenia B.
   and Shagidullin, Rifgat R. and Gainutdinov, Marat Kh.},
Title = {Opposite effects of moderate heat stress and hyperthermia on cholinergic
   system of soil nematodes <i>Caenorhabditis elegans</i> and
   <i>Caenorhabditis briggsae</i>},
Journal = {JOURNAL OF THERMAL BIOLOGY},
Year = {2016},
Volume = {62},
Number = {A},
Pages = {37-49},
Month = {DEC},
Abstract = {Cholinergic system plays important role in all functions of organisms of
   free-living soil nematodes C. elegans and C. briggsae. Using
   pharmacological analysis we showed the existence of two opposite
   responses of nematodes cholinergic system to moderate and extreme heat
   stress. Short-term (15 min) noxious heat (31-32 degrees C) caused
   activation of cholinergic synaptic transmission in C. elegans and C.
   briggsae organisms by sensitization of nicotinic ACh receptors. In
   contrast, hyperthermia blocked cholinergic synaptic transmission by
   inhibition of ACh secretion by neurons. The resistance of behavior to
   extreme high temperature (36-37 degrees C) was significantly higher in
   C. briggsae than in C. elegans, and thermostability of cholinergic
