@article{ WOS:000579864500045,
Author = {Ashaf-Ud-Doulah, Mohammad and Al Mamun, Abdullah and Rahman, Mohammad
   Lutfar and Islam, S. M. Majharul and Jannat, Rayeda and Hossain, Mostafa
   Ali Reza and Shahjahan, Md},
Title = {High temperature acclimation alters upper thermal limits and growth
   performance of Indian major carp, rohu, <i>Labeo rohita</i> (Hamilton,
   1822)},
Journal = {JOURNAL OF THERMAL BIOLOGY},
Year = {2020},
Volume = {93},
Month = {OCT},
Abstract = {Increase in water temperature due to anthropogenic and climatic changes
   is expected to affect physiological functions of fish. In this study, we
   determined high temperature tolerance (CTmax) of a common aquacultured
   Indian major carp, rohu, Labeo rohita fingerlings (15.96 +/- 0.72 g BW,
   11.56 +/- 0.42 cm TL) followed by acclimatization at three temperatures
   (30, 33, 36 degrees C). To determine the CTmax, we analyzed the major
   hemato-biochemical indices - hemoglobin (Hb), red blood cell (RBC),
   white blood cell (WBC), blood glucose levels, and erythrocytic nuclear
   abnormalities (ENAs) and erythrocytic cellular abnormalities (ECAs) of
   peripheral erythrocytes in the fish sampled at the start and end point
   at each acclimated temperature. Significantly decreased CTmax of the
   fish was found at 36 degrees C compared to 30 degrees C and 33 degrees
   C. The fish in the highest (36 degrees C) temperature were found with
   significantly lower Hb and RBC content and significantly higher WBC and
   blood glucose levels than that of the fishes in the lowest (30 degrees
   C) temperature both at the start and end points. The highest frequencies
   of ENAs and ECAs were found in the highest (36 degrees C) temperature
   group compared to the lowest (30 degrees C) temperature group at both
   the points. We also evaluated growth performance of the rohu fingerlings
   reared in the three temperatures for 60 days. The growth parameters -
   final weight gain, percent weight gain and specific growth rate were the
   highest at 33 degrees C and the lowest at 36 degrees C. The present
   study revealed that the highest temperature (36 degrees C) tested here
   may be hazardous to rohu and the temperature should be kept below 36
   degrees C in the aquaculture setting to avoid physiological damage and
   growth and production loss to the fish.},
