@article{ WOS:000379374800013,
Author = {Kakitsuba, Naoshi},
Title = {Physiological responses to changes in relative humidity under thermally
   neutral, warm and hot conditions},
Journal = {JOURNAL OF THERMAL BIOLOGY},
Year = {2016},
Volume = {59},
Pages = {86-91},
Month = {JUL},
Abstract = {Four hypothetical thermophysiological responses to changes in relative
   humidity (Rh) under thermally neutral, warm, and hot conditions were
   proposed for a person at rest. Under thermally neutral and warm
   conditions, the first hypothetical response to an increase in Rh was a
   decrease in mean skin temperature ((T) over bar (sk)) due to increase in
   mean evaporation rate ((E) over bar (sk)), and the second hypothetical
   response to a decrease in Rh was a decrease, an increase, or no change
   in (T) over bar (sk), depending on changes in the (E) over bar (sk).
   Under hot conditions, the third hypothetical response to an increase in
   the Rh was an increase in (T) over bar (sk) or decrease in (T) over bar
   (sk) upon decrease in the Rh due to changes in (E) over bar (sk), and
   the forth hypothetical response to an increase in Rh was an increase in
   (T) over bar (sk) due to increase in the peripheral blood flow rate
   (SkBF). To test these hypotheses, the (T) over bar (sk) and (E) over bar
   (sk) of four young male volunteers were measured at 28 degrees C, 30
   degrees C, or 32 degrees C while the Rh was maintained at 40\% or 80\%
   Rh for 60 min after 20 min exposure at 60\% Rh (control condition). In a
   second experiment, the (T) over bar (sk), (E) over bar (sk), and SkBF of
   five young male volunteers were measured at 34 degrees C-40\% Rh or 36
   degrees C-40\% Rh, or 34 degrees C-70\% Rh or 36 degrees C-70\% Rh for
   60 min after 20 min exposure at 28 degrees C-60\% Rh (control
   condition). The first hypothesis was partly supported by the findings
   that the (T) over bar (sk) was lower than the control values at 28
   degrees C-80\% Rh and the (E) over bar (sk) was higher than the control
   values at 80\% Rh at any tested temperature. The second hypothesis was
   partly supported by the findings that the (T) over bar (sk) was lower
   than the control values at 28 degrees C-40\% Rh, and there were small
   changes in both (T) over bar (sk) and (E) over bar (sk) at 30 degrees
   C-40\% Rh. The third and fourth hypotheses were supported by the
   findings that the (T) over bar (sk) at 36 degrees C-70\% Rh was
   significantly higher (p < 0.01) than at 36 degrees C-40\% Rh, the
   <(E)over bar>(sk) was significantly higher (p < 0.01) at 70\% Rh than at
   40\% Rh, and SkBF was positively correlated with <(T)over bar>(sk). (C)
   2016 Elsevier Ltd. All rights reserved.},
